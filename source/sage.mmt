namespace http://www.sagemath.org/

import MitM http://mathhub.info/MitM/

theory Types : ur:?PLF =
	object : type 
	prop : type 
	ded : prop → type  ## ⊦ 1 
	structural : type 
	structureof : structural → type  ## ≤ 1 


theory Convenient = 
  include ur:?PLF 
  include http://gl.mathhub.info/MMT/LFX/Records?LFRecords 

	
theory PythonBasicTypes : ?Convenient =  
  python_type : type 
  pyMMT : python_type → type  
  pylist : python_type → python_type
  pysequence : python_type → python_type 
  pystr : python_type 
  pyint : python_type 
  pyfloat : python_type 
  pytype : python_type 
  pydict: pyMMT pystr → python_type


theory PythonFunction : ?Convenient = 
  include ?PythonBasicTypes 
  theory PythonFunctionTheory = 
    name : pyMMT pystr 
    docstring : pyMMT pystr 
  
  FromTheory pyfunction : PythonFunctionTheory 


theory PythonClass : ?Convenient = 
  include ?PythonBasicTypes 
  theory PythonClassTheory =
    superclasses : pyMMT (pysequence pyfunction) 
    
    docstring : pyMMT pystr 
    name : pyMMT pystr 
    methods : pyMMT (pysequence pyfunction) 
  FromTheory pyclass : PythonClassTheory 
  

theory Python : ?Convenient = 
  include ?PythonBasicTypes 
  include ?PythonFunction 
  include ?PythonClass 
  
  

theory SageConventionParent : ?Python =
  include MitM:Foundation?Math 


  
theory Sage : ?Python = 
  include ?SageConventionParent  
  sage_type : type 
  sageMMT : sage_type → type 
  sagepy : sage_type → python_type
 

theory MathObject : ur:?PLF = 
  include MitM:Foundation?Math 
  math_type : type  
  mathMMT : math_type → type 

  
theory SageObject : ?Sage =
  include ?MathObject 
  implements :  sageMMT sage_type → math_type # 1 implements 2 


theory SageLiterals : ?Sage = 
  include ?SageObject 
  include MitM:Foundation?Literals   



// class TransitiveGroup(PermutationGroup_unique):
//    def __init__(self, d, n):
    
  
theory SageTransitiveGroup : ?Sage = 
  include ?SageLiterals 
  include MitM:smglom/permutationgroup?transitive_group_action 
  theory TransitiveGroupImplementationTheory =
    TransitiveGroup : python_type = transitivegroupaction
    init : pyMMT Integer → pyMMT PositiveInteger → pyMMT TransitiveGroup
  
  FromTheory TransitiveGroupImplementation : TransitiveGroupImplementationTheory
